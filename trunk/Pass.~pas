{$R *.dfm}

procedure TPassCra.Button1Click(Sender: TObject);
type Tnum = array[1..9] of integer;

var
num:Tnum;
i:integer;
p:integer;
text:string;
begin
text:='';
p:=0;
randomize;
for p:=1 to 10 do
begin
i:=random(9);
Edit1.Text:=IntTostr(num[i])+text;
end;







end;

end.
